def main():
   celsius = eval(input("Wha is the Celsius tempature? "))
   fahrenheit = 9/5 * celsius + 32
  
   if fahrenheit > 90:
      print("Heat Warning")
   elif fahrenheit < 30:
      print("Cold Warning")
   else:
      print("")  
       
   print("The tempature is", fahrenheit, "degrees fahrenheit.")



main()
